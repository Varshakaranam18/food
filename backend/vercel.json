{
    "version": 2,
    "builds": [
      { "src": "backend/server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/food/(.*)", "dest": "/backend/server.js" },
      { "src": "/api/user/(.*)", "dest": "/backend/server.js" },
      { "src": "/api/cart/(.*)", "dest": "/backend/server.js" },
      { "src": "/api/order/(.*)", "dest": "/backend/server.js" },
      { "src": "/images/(.*)", "dest": "/backend/server.js" },
      { "src": "/(.*)", "dest": "/backend/server.js" }
    ]
  }
  